class HelloWorld {
    public static void main(String[] args) {
    int n1= 24;
    int n2 = 0;
    
    if(n2==0){
        int gcd =  n1;
        System.out.println(gcd);
    }
    else{
      while(n1% n2 != 0 ){
        int rem = n1 % n2;
        n1 = n2;
        n2= rem;
    }
     int gcd = n2;
    System.out.println(gcd);
    }   
    }
    
   
} 
